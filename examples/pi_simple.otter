fn leibniz_pi(iterations: float) -> float:
    let pi = 0.0
    let sign = 1.0
    for n in 0..iterations:
        let term = sign / (2.0 * n + 1.0)
        pi += term
        sign = -sign
    return 4.0 * pi

fn main():
    print("Starting pi calculation...")
    let iterations = 1000.0
    let result = leibniz_pi(iterations)
    print("Pi calculation complete!")
